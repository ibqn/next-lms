{
  "name": "database",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "tsx --env-file-if-exists=.env src/index.ts",
    "seed": "pnpm run seed:categories && pnpm run seed:users",
    "seed:categories": "tsx --env-file-if-exists=.env src/seed-categories.ts",
    "seed:users": "tsx --env-file-if-exists=.env src/seed-users.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "argon2": "^0.44.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "lodash.unset": "^4.5.2",
    "postgres": "^3.4.8",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/lodash.unset": "^4.5.9",
    "@types/node": "^25.3.0",
    "drizzle-kit": "^0.31.9",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
